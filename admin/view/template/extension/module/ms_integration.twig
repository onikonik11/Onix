                    <li><a href="#tab-category" data-toggle="tab">{{ tab_category }}</a></li>
                    <li><a href="#tab-orders" data-toggle="tab">{{ tab_orders }}</a></li>
                    <li><a href="#tab-import" data-toggle="tab">{{ tab_import }}</a></li>
                    {% endif %}
